<div class="container">
  <div class="top-question">
    <h1>Latest Question</h1>

      <p>Q: <%= link_to(@questions.last.title, question_path(@questions.last)) %></br>
        <%= @questions.last.body %></p>
    </div>

      <div class="other-questions">
        <% @questions.reverse.each do |question| %>
        <!-- <h3><#%= link_to(question.title, question_path)%></h3> -->
        <h3><%= link_to image_tag("1434948365_ic_keyboard_arrow_up_48px.svg"), like_question_path(question), method: :put %><%= link_to image_tag("1434948582_ic_keyboard_arrow_down_48px.svg"), dislike_question_path(question), method: :put %> (<%= question.score %>) <%= link_to(question.title, question_path(question))%></h3>
        <p><%= question.body %></p>
        <% end %>
      </div>
    </div>

    <% if current_user %>
    <%= link_to("Ask a Question", new_question_path) %>
    <% end %>

  </div>

  <hr>
  <h2>Tags:</h2>
  <% @tags.uniq.each do |tag| %>
  <div style="border:1px solid black; margin:5px; display:inline-block; padding:5px; border-radius:3px; background:#f2f2f2;">
    <%= link_to tag.name, tag_path(tag) %>
  </div>
  <% end %>
