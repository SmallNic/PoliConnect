<div class="container">
  <div class="top-question">
    <h1>Latest Question</h1>
    <p>Q: <% @questions.last %></br>
    <% @questions.last %>
  </div>

  <div class="other-questions">
    <% @questions.each do |question| %>
      <h3><%= link_to(question.title, question_path(question))%></h3>
      <p><%= question.body %></p>
    <% end %>
  </div>
</div>

<%= link_to("Ask a Question", new_question_path) %>

<div style="border:1px solid black; margin-top:20px;">
Tag Box:
<% @tags.uniq.each do |tag| %>
  <%= link_to tag.name, tag_path(tag) %>
<% end %>
</div>
